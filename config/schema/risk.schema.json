{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Risk",
  "type": "object",
  "required": [
    "id",
    "name",
    "category",
    "description"
  ],
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "category": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "subrisks": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "units": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "factors": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "questions": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "sources": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  }
}